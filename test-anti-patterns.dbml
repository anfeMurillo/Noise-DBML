// Test file for Anti-Pattern Detection
// Este archivo contiene un esquema simple para probar la funcionalidad

// ✅ Good table design
Table users {
  id integer [pk, increment]
  username varchar(50) [unique, not null]
  email varchar(100) [unique, not null]
  created_at timestamp [not null, default: `now()`]
  
  Note: 'User accounts table with proper design'
}

// ⚠️ Table without primary key
Table logs {
  timestamp timestamp
  message text
  level varchar(10)
  
  Note: 'This will trigger a warning - no PK'
}

// ⚠️ Too many nullable fields
Table optional_data {
  id integer [pk, increment]
  field1 varchar
  field2 varchar
  field3 varchar
  field4 varchar
  field5 varchar
  field6 varchar
  
  Note: '6 of 7 fields are nullable - should trigger warning'
}

